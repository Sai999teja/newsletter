<div{{#if edit}} mc:edit="{{ edit }}"{{/if}}>
  <a href="{{ default href '#' }}" class="link {{#if white}}link--white{{/if}}">
    <img class="link__caret" src="images/caret{{#if white}}-white.png{{else}}.jpg{{/if}}" width="5" height="9" alt="" />
    &nbsp;
    <span class="link__text">
      {{> @partial-block}}
    </span>
    {{#if spotify}}
      <img class="link__icon" src="images/social-icons/spotify.png" width="16" height="16" alt="">
    {{/if}}
  </a>
</div>
